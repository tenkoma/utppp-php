{
    "name": "tenkoma/utppp-example",
    "type": "project",
    "authors": [
        {
            "name": "Koji Tanaka",
            "email": "tenkoma@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tenkoma\\UtpppExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tenkoma\\UtpppExample\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">= 8.2.0",
        "ramsey/collection": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse --configuration=phpstan.neon",
        "phpcs": "phpcs -s .",
        "phpcbf": "phpcbf ."
    }
}
